# Bridge service port
PORT=3001

# Google Calendar
GOOGLE_CALENDAR_ID=your-calendar-id@group.calendar.google.com

# Google Sheets
GOOGLE_SHEET_ID=your-google-sheet-id
GOOGLE_SHEET_TAB=Bookings

# Auth method: "service_account" or "oauth"
GOOGLE_AUTH_METHOD=service_account

# Service account path (if GOOGLE_AUTH_METHOD=service_account)
GOOGLE_SERVICE_ACCOUNT_KEY_PATH=./service-account.json

# OAuth Desktop credentials (if GOOGLE_AUTH_METHOD=oauth)
GOOGLE_OAUTH_CLIENT_ID=
GOOGLE_OAUTH_CLIENT_SECRET=
GOOGLE_OAUTH_REDIRECT_URI=urn:ietf:wg:oauth:2.0:oob

# Pricing (RON)
PRICE_AUTO=200
PRICE_GPL=250
PRICE_UTIL=300
